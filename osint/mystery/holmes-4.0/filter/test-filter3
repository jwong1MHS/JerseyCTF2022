b = 1+2*3^4;
c = 2 + toint("13");
a = 1*c + a + 0 + b + c^(3- 2) + a^0;
if 3 < 3 { log "1"; } else { log "2"; } undef { log "3"; }
if !! (defined(x) && defined(z)) { log "a"; }
elif (defined(x) || defined(z)) && true { log "b"; }
else { log "c"; }

switch (1+1)
{
	case ==1:
		log 1;
	case ==2:
		log 2;
	case ==3:
		log 3;
}
xa = "";
switch xa . ("ahoj" . "3")
{
	case == "ahoj1":
